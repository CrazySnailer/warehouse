<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.calf.framework.warehouse.entity.TbWhUpD" table="tb_wh_up_d">
    
        <id name="upDId" type="java.lang.Long">
            <column name="UP_D_ID" precision="12" scale="0" not-null="true"/>
            <generator class="sequence">
                <param name="sequence">SEQ_WH_UP_D</param>
            </generator>
      	 </id>
		
				 
       <property name="upId" type="java.lang.Long">
            <column name="UP_ID" precision="12" scale="0"/>
        </property>
		
				 
       <property name="productId" type="java.lang.Long">
            <column name="PRODUCT_ID" precision="12" scale="0"/>
        </property>
		
				 
       <property name="inspectionDId" type="java.lang.Long">
            <column name="INSPECTION_D_ID" precision="12" scale="0"/>
        </property>
		
				 
       <property name="structId" type="java.lang.Long">
            <column name="STRUCT_ID" precision="12" scale="0"/>
        </property>
		
				 
       <property name="qty" type="java.lang.Long">
            <column name="QTY" precision="12" scale="0" not-null="true"/>
        </property>
		
				 
		<property name="upType" type="java.lang.String">
            <column name="UP_TYPE" length="2"/>
        </property>
		
				 
       <property name="createUser" type="java.lang.Long">
            <column name="CREATE_USER" precision="12" scale="0" not-null="true"/>
        </property>
		
				 
		<property name="createDate" type="java.util.Date">            
            <column name="CREATE_DATE" length="7" not-null="true"/>
        </property>
		
				 
       <property name="updateUser" type="java.lang.Long">
            <column name="UPDATE_USER" precision="12" scale="0"/>
        </property>
		
				 
		<property name="updateDate" type="java.util.Date">            
            <column name="UPDATE_DATE" length="7"/>
        </property>
    </class>
</hibernate-mapping>