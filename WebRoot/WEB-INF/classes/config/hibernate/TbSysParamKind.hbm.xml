<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.calf.framework.um.entity.TbSysParamKind" table="TB_SYS_PARAM_KIND">
    
        <id name="kindCode" type="java.lang.String">
            <column name="KIND_CODE" length="50" not-null="true"/>            
      	 </id>
				 
		<property name="kindName" type="java.lang.String">
            <column name="KIND_NAME" length="100" not-null="true"/>
        </property>
				 
		<property name="dataStatus" type="java.lang.String">
            <column name="DATA_STATUS" length="2" not-null="true"/>
        </property>
				 
       <property name="createUser" type="java.lang.Long">
            <column name="CREATE_USER" precision="12" scale="0"/>
        </property>
				 
		<property name="createDate" type="java.util.Date">            
            <column name="CREATE_DATE" length="7"/>
        </property>
				 
       <property name="updateUser" type="java.lang.Long">
            <column name="UPDATE_USER" precision="12" scale="0"/>
        </property>
				 
		<property name="updateDate" type="java.util.Date">            
            <column name="UPDATE_DATE" length="7"/>
        </property>
    </class>
</hibernate-mapping>